    "input ('write a number between 1 and 10')\n",
    "for int in range(10):\n",
    "    # use a if the number is a multiple of 3, otherwise use b\n",
    "    message = \"Good Morning\"\n",
    "    if (int % 3) == 0:\n",
    "        message = message + \" Mr a\"\n",
    "    else:\n",
    "        message = message + \" Mr b\"\n",
    "print(message)\n"
